#!/usr/bin/env python

import cPickle

import sys

# check there's one argument
if (len(sys.argv)!=2):
    print "Usage: popheader [pop model]"
    sys.exit()

if sys.argv[1] == "-h" or sys.argv[1] == "--help":
    print "Usage: popheader [pop model]"
    sys.exit()


# load the model
with open(sys.argv[1], 'rb') as f:
    pop = cPickle.load(f)

print pop
